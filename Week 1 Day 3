      "source": [
        "obj_list = [11.25, 18.0, 20.0, 10.75, 9.50]\n",
        "def mean_list(inp_list):\n",
        "    # isikan code\n",
        "    total_list = sum(obj_list)\n",
        "    total_item_list = len(obj_list)\n",
        "    hasil = total_list / total_item_list\n",
        "    return(hasil)\n",
        "    \n",
        "print(mean_list(obj_list))"
      ],
